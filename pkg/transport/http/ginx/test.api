syntax = "v1"

type UserV1 {
    Name string `json:"name"`
}

type UserV2 {
    Name string `json:"name"`
}

type GetUser {
    Name string `json:"name"`
}

@server (
    prefix: /v1
)

service user-api {
    @handler usersv1
    get /users (GetUser) returns ([]UserV1)
}

@server (
    prefix: /v2
)

service user-api {
    @handler usersv2
    get /users returns ([]UserV2)
}